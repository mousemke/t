{
  "name": "t",
  "version": "0.0.5",
  "description": "A utility to parse strings and retrieve translations",
  "main": "./dist/t.cjs",
  "exports": {
    "basic": "./dist/t.js",
    "import": "./dist/t.mjs",
    "require": "./dist/t.cjs"
  },
  "types": "dist/t.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && rollup src/index.ts --config rollup.config.js",
    "test:lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "prepack": "npm run build",
    "prettier": "prettier --write 'src/**/*.{js,ts}'",
    "test": "jest --coverage",
    "test:coverage": "jest --coverage && if [[ $OSTYPE == darwin* ]]; then open ./coverage/lcov-report/index.html; else xdg-open ./coverage/lcov-report/index.html; fi",
    "test:types": "tsc",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git@github.bus.zalan.do:ephemerals/t.git"
  },
  "contributors": [
    {
      "name": "Sadie Braun",
      "email": "sadie@knoblau.ch"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-typescript": "^8.2.3",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "commitlint": "^13.1.0",
    "eslint": "^7.31.0",
    "eslint-plugin-import": "^2.23.4",
    "husky": "^7.0.0",
    "jest": "^28.1.3",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.55.0",
    "rollup-plugin-dts": "^3.0.2",
    "ts-jest": "^28.0.7",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --ext .js,.jsx,.ts,.tsx"
    ]
  }
}
